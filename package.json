{
    "name": "selenium-mocha-digital-skola",
    "version": "1.0.0",
    "description": "selenium webdriver test using Mocha with Mochawesome reports",
    "scripts": {
        "test": "mocha test/sesi-9/test-sauce.js --recursive --timeout 60000",
        "test-sesi9": "mocha test/sesi-9/test_sesi9.js --recursive --timeout 60000",
        "test-parallel": "mocha test --parallel --timeout 60000",
        "test-pertama": "mocha tests --grep 'Pertama' --recursive --timeout 60000",
        "jalanin-mochawesome": "mocha test/sesi-9/test-sauce.js --recursive --timeout 60000 --reporter mochawesome",
        "jalanin-mochawesome-html": "mocha test --recursive --timeout 60000 --reporter mocha-simple-html-reporter --reporter-options output=reporter.html"
    },
    "dependencies": {
        "chromedriver": "^135.0.2",
        "selenium-webdriver": "^4.31.0"
    },
    "devDependencies": {
        "mocha": "^10.2.0",
        "mocha-simple-html-reporter": "2.0.0",
        "mochawesome": "^7.1.3"
    }
}
